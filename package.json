{
  "name": "ai-agent-art-forge",
  "version": "1.0.0",
  "description": "An autonomous AI agent that creates, publishes, and promotes NFT artwork using AI generation and social media integration",
  "main": "art-forge.ts",
  "scripts": {
    "start": "NODE_OPTIONS='--no-warnings' ts-node ./art-forge.ts",
    "dev": "NODE_OPTIONS='--no-warnings' nodemon --exec ts-node ./art-forge.ts",
    "build": "tsc",
    "interactive": "NODE_OPTIONS='--no-warnings' ts-node ./art-forge.ts --interactive",
    "test:art": "ts-node tests/integration/test-art-generation.ts",
    "test:ipfs": "ts-node tests/integration/test-ipfs.ts",
    "test": "npm run test:art && npm run test:ipfs"
  },
  "dependencies": {
    "@coinbase/cdp-agentkit-core": "^0.0.11",
    "@coinbase/cdp-langchain": "^0.0.12",
    "@langchain/core": "^0.3.30",
    "@langchain/langgraph": "^0.2.39",
    "@langchain/openai": "^0.3.14",
    "@openzeppelin/contracts": "^5.0.1",
    "@pinata/sdk": "^2.1.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "openai": "^4.78.1",
    "sharp": "^0.33.5",
    "twitter-api-v2": "^1.16.0",
    "web3": "^4.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "hardhat": "^2.20.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
